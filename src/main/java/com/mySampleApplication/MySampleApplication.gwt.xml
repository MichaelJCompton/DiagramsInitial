<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="MySampleApplication">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <inherits name='com.google.gwt.user.theme.standard.Standard'/>

    <!-- The graphics library -->
    <!-- <inherits name='com.emitrom.lienzo.Lienzo' /> -->
    <inherits name="com.ait.lienzo.Lienzo"/>

    <inherits name="michael.com.AbstractSyntax"/>


    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.mySampleApplication.client.MySampleApplication'/>


    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>


</module>
